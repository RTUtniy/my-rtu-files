{\large\textbf{Teorētisko ātrumu kļūdas}}\\[1em]
\textbf{a) Teorētiskais lodītes kustības ātrums, ievērojot lodītes rotāciju}\\
v_{v+r}=\sqrt{\frac{10}7gH}\\
\Delta v_{v+r}=\dfrac{\partial v_{v+r}}{\partial H}\*\Delta H\\
\dfrac{\partial v_{v+r}}{\partial H}=\sqrt{\frac{10}7g}\*\frac1{2\sqrt{\overline H}}\\
\Delta H=\sqrt{\Delta H_\text{gad.}^2+\Delta H_\delta^2}\\
\Delta H_\text{gad.}=s_H\*t_\beta(5)=0,0757\cdot2,78\approx0,2104\\
\Delta H_\delta=\frac\delta3\*t_\beta(\infin)=\frac1{3000}\*1,96\approx6,53\*10^{-4}\\
\Delta H=\sqrt{0,2104^2+(6,53\*10^{-4})^2}\approx0,2104\\
\Delta v_{v+r}=\sqrt{\frac{10}79,807}\frac1{2\sqrt{0,451}}\*0,2104\approx0,5863\measure{m/s}\\

\boxed{v_{v+r}=2,4762\pm0,5863\measure{m/s}}\\[0.5em]


\textbf{b) Teorētiskais lodītes kustības ātrums, neievērojot lodītes rotāciju}\\
v_v=\sqrt{2gH}\\
\Delta v_v=\dfrac{\partial v_v}{\partial H}\*\Delta H\\
\dfrac{\partial v_v}{\partial H}=\sqrt{0,5\frac g{\overline H}}\\
\Delta v_v=\sqrt{0,5\frac{9,807}{0,451}}\*0,2104\approx0,6938\measure{m/s}\\

\boxed{v_{v}=2,9299\pm0,6938\measure{m/s}}\\[1em]

